{
    "name": "nickmel/laravel-smsto",
    "description": "A Laravel package to hook up to the SMS.to REST API",
    "keywords": [
        "sms",
        "marketing",
        "notifications",
        "api",
        "REST",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicholas Meletiou",
            "email": "nicholas.mel@icloud.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^5.8",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Nickmel\\SMSTo\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nickmel\\SMSTo\\SMSToServiceProvider"
            ],
            "aliases": {
                "SMSTo": "Nickmel\\SMSTo\\SMSToFacade"
            }
        },
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
