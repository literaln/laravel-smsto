{
    "name": "nickmel/laravel-sms.to",
    "description": "A Laravel package to hook up to the SMS.to REST API",
    "keywords": [
        "sms",
        "marketing",
        "notifications",
        "api",
        "REST",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicholas Meletiou",
            "email": "nicholas.mel@icloud.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.8",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "nickmel\\SMSTo\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "nickmel\\SMSTo\\SMSToServiceProvider"
            ],
            "aliases": {
                "SMSTo": "nickmel\\SMSTo\\Facade"
            }
        }
    }
}
